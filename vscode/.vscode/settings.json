{
    "files.exclude": {
        "node_modules/": true,
        ".vscode-test/": true,
        "out-test/": true,
        "dist/": true
    },
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.fixAll": "explicit"
    },
    "search.exclude": {
        "dist": true,
        "package-lock.json": true
    },
    "[javascript]": {
        "editor.formatOnSave": false
    },
    "[typescript]": {
        "editor.formatOnSave": false
    }
}
